{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"sourceMap": true,
		"outDir": "../build/",
		"moduleResolution": "node",
		"resolveJsonModule": true
	},
	"files": [
		"../@types/mathjax-node.d.ts",
		"index.ts"
	],
	"typeAcquisition": {
		"enable": true
	}
}
